    "editorCursor.foreground": "#2563EB",
    "editorLineNumber.foreground": "#9CA3AF",
    "editorLineNumber.activeForeground": "#0F2438",
    "editorGutter.background": "#FAFBFC",
    "editorWhitespace.foreground": "#D1D5DB",
    "editorIndentGuide.background1": "#E2E8F0",
    "editorIndentGuide.activeBackground1": "#CBD5E1",
    "editor.wordHighlightBackground": "#DBEAFE80",
    "editor.wordHighlightStrongBackground": "#DBEAFE80",
    "editor.findMatchBackground": "#FEF08A",
