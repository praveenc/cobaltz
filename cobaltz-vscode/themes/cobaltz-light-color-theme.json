    "sideBar.background": "#F1F5F9",
    "sideBar.foreground": "#0F2438",
    "sideBarTitle.foreground": "#0F2438",
    "sideBarSectionHeader.background": "#E8ECF0",
    "sideBarSectionHeader.foreground": "#0F2438",

    "list.activeSelectionBackground": "#DBEAFE",
    "list.activeSelectionForeground": "#0F2438",
    "list.hoverBackground": "#E2E8F0",
    "list.focusBackground": "#DBEAFE",
    "list.inactiveSelectionBackground": "#E2E8F0",

    "titleBar.activeBackground": "#E8ECF0",
    "titleBar.activeForeground": "#0F2438",
    "titleBar.inactiveBackground": "#F1F5F9",
    "titleBar.inactiveForeground": "#6B7280",

    "statusBar.background": "#3478C6",
    "statusBar.foreground": "#FFFFFF",
